{
    "name": "taotie-workbench",
    "version": "1.0.0",
    "description": "The workbench for coder",
    "private": true,
    "scripts": {
        "start:workbench": "yarn workspace workbench-electron start",
        "start:views": "yarn workspace views start"
    },
    "workspaces": [
        "packages/*"
    ],
    "files": [
        "packages"
    ],
    "devDependencies": {
        "lint-staged": "^9.2.5",
        "husky": "^4.0.0",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-import": "^1.13.0",
        "@ecomfe/eslint-config": "^3.2.0",
        "@commitlint/cli": "^8.3.4",
        "@commitlint/core": "^8.3.4",
        "@commitlint/travis-cli": "^8.3.5",
        "commitizen": "^4.0.3",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.1",
        "typescript": "^4.0.3"
    },
    "lint-staged": {
        "!(example|dist|output|__mocks__)/**/*.(ts|js)": [
            "eslint"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
          "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/BUPTlhuanyu/taotie-workbench"
    },
    "author": "lhuanyu <lhuanyutot@163.com>",
    "license": "MIT"
}
